{
  "name": "react-tenor",
  "version": "1.1.0",
  "description": "Integrate with the Tenor GIF API",
  "main": "dist/Tenor.js",
  "scripts": {
    "build": "yarn build:js && yarn build:docs && yarn build:css",
    "build:js": "babel src --out-dir dist --ignore '**/__tests__'",
    "build:docs": "webpack --mode production",
    "build:css": "cp src/styles.css dist/styles.css",
    "lint": "eslint .",
    "start": "webpack-dev-server --mode development --hot",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CultureHQ/react-tenor.git"
  },
  "author": "Kevin Deisz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CultureHQ/react-tenor/issues"
  },
  "homepage": "https://github.com/CultureHQ/react-tenor#readme",
  "peerDependencies": {
    "react": "^16",
    "react-dom": "^16"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-env": "^7.4.1",
    "@babel/preset-react": "^7.0.0",
    "@culturehq/eslint-config": "^1.0.2",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "eslint": "^5.15.3",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^24.5.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-testing-library": "^6.0.1",
    "style-loader": "^0.23.0",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./setupTests.js"
    ],
    "testRegex": ".test.js"
  }
}
