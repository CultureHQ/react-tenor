{
  "name": "react-tenor",
  "version": "1.2.0",
  "description": "Integrate with the Tenor GIF API",
  "main": "dist/Tenor.js",
  "scripts": {
    "build": "yarn build:js && yarn build:docs && yarn build:css",
    "build:js": "babel src --out-dir dist --ignore '**/__tests__'",
    "build:docs": "webpack --mode production",
    "build:css": "cp src/styles.css dist/styles.css",
    "lint": "chq-scripts lint",
    "start": "webpack-dev-server --mode development --hot",
    "test": "chq-scripts test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CultureHQ/react-tenor.git"
  },
  "author": "Kevin Deisz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CultureHQ/react-tenor/issues"
  },
  "homepage": "https://github.com/CultureHQ/react-tenor#readme",
  "peerDependencies": {
    "react": "^16",
    "react-dom": "^16"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@culturehq/scripts": "^0.2.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.1",
    "isomorphic-fetch": "^2.2.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-testing-library": "^7.0.1",
    "style-loader": "^0.23.0",
    "webpack": "^4.32.1",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.4.1"
  }
}
